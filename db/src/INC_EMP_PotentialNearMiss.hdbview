VIEW INC_EMP_PotentialNearMiss AS
SELECT PN.NMPID,PN.INCID,PN.NRMID,PN.NRMPT,MD.TXVAL AS NRMPTTXT, PN.ISDEL 
FROM "INC_T_NMPOT" AS PN
INNER JOIN "INC_T_INCDT" AS INC ON INC.INCID = PN.INCID AND INC.ISDEL = '0' AND PN.ISDEL = '0'
LEFT JOIN "INC_M_MSTDT" AS MD ON MD.UNQID = PN.NRMPT AND MD.TYVAL = 'Potential Near Miss' AND MD.ISDEL='0'
WHERE PN.ISDEL='0';