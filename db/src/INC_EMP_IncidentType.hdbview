VIEW INC_EMP_IncidentType AS
SELECT INCTP.ICTID,INCTP.INCID,INCTP.INCTY,INCTP.ISDEL,MSL.TXVAL INCTYTXT
FROM "INC_T_INCTP" INCTP
INNER JOIN "INC_T_INCDT" AS INC ON INC.INCID=INCTP.INCID AND INC.ISDEL='0'
LEFT JOIN "INC_M_MSTDT" AS MSL ON MSL.UNQID=INCTP.INCTY AND MSL.TYVAL='Incident Type' AND INCTP.ISDEL='0'
WHERE INCTP.ISDEL='0';