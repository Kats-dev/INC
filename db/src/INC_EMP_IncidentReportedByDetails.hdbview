VIEW INC_EMP_IncidentReportedByDetails AS
SELECT RP.REPID,RP.INCID,RP.EMPNM,RP.EMPID,RP.DEPNM,RP.POSIT,RP.MGRID,RP.MGRNM,RP.EMAIL,
RP.CSTM1,RP.CSTM2,RP.CSTM3,RP.CSTM4,RP.CSTM5,RP.CSTM6,RP.CSTM7,RP.CSTM8
FROM "INC_T_REPBY" RP
INNER JOIN "INC_T_INCDT" AS INC ON INC.INCID=RP.INCID AND INC.ISDEL='0'
WHERE RP.ISDEL='0';