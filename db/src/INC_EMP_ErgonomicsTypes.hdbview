VIEW INC_EMP_ErgonomicsTypes AS 
SELECT ER.EGTID,ER.ERGID,ER.INCID,ER.ERTYP,MD.TXVAL AS ERTYPTXT,ER.ISDEL
FROM "INC_T_ERGTY" AS ER
INNER JOIN "INC_T_INCDT" AS INC ON INC.INCID = ER.INCID AND INC.ISDEL = '0' AND ER.ISDEL = '0'
LEFT JOIN "INC_M_MSTDT" AS MD ON MD.UNQID = ER.ERTYP AND MD.TYVAL = 'Types Of Ergonomics' AND MD.ISDEL='0'
WHERE ER.ISDEL='0';