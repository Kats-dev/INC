VIEW INC_EMP_WorkPlaceInjury AS
SELECT WP.WPIID, WP.INCID, WP.UNQID, WP.INJIL, WP.FACNM, WP.MEDFC, WP.DOCNM, WP.INJDS, WP.INJPS, WP.ISALT, WP.ALTOP, 
WP.ANWAB, WP.DIMAB, WP.IPLID,WP.CSTM1,WP.CSTM2,WP.CSTM3,WP.CSTM4,WP.CSTM5,WP.CSTM6,WP.CSTM7,WP.CSTM8,
WP.ISREC,WP.ISREP
FROM "INC_T_WPIDT" AS WP
INNER JOIN "INC_T_INCDT" AS INC ON INC.INCID = WP.INCID AND INC.ISDEL = '0' AND WP.ISDEL = '0'
WHERE WP.ISDEL='0';