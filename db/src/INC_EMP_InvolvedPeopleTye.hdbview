VIEW INC_EMP_InvolvedPeopleType AS
SELECT PY.IPTID, PY.IPLID, PY.INCID, PY.INVTY, MD.TXVAL INVTY_TXT
FROM "INC_T_IVPTY" PY
INNER JOIN "INC_T_INCDT" AS INC ON INC.INCID = PY.INCID AND INC.ISDEL = '0' AND PY.ISDEL = '0'
LEFT JOIN "INC_M_MSTDT" AS MD ON MD.UNQID = PY.INVTY AND MD.TYVAL='Person Type' AND MD.ISDEL='0'
WHERE PY.ISDEL='0';