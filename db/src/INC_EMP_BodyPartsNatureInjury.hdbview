VIEW INC_EMP_BodyPartsNatureInjury AS
SELECT BD.BDPID,BD.WPIID,BD.INCID,BD.BDPRT,BD.NAINJ,BD.ISIDE,BD1.TXVAL UNQID_TXT,BD2.TXVAL NATID_TXT,BD3.TXVAL SIDID_TXT,BD.BDPDS
FROM "INC_T_BDPRT" AS BD
LEFT JOIN "INC_M_BDPRT" AS BD1 ON BD1.UNQID=BD.BDPRT AND BD1.TYVAL = 'BodyPart' AND BD1.ISDEL='0'
LEFT JOIN "INC_M_BDPRT" AS BD2 ON BD2.UNQID=BD.NAINJ AND  BD2.TYVAL = 'NatureOfInjury' AND BD2.ISDEL='0'
LEFT JOIN "INC_M_BDPRT" AS BD3 ON BD3.UNQID=BD.ISIDE AND BD3.TYVAL = 'InjurySide' AND BD3.ISDEL='0'
WHERE BD.ISDEL='0'; 