VIEW INC_EMP_NearMissType AS
SELECT NT.NMTID,NT.INCID,NT.NRMID,NT.NRMTP,MD.TXVAL AS NRMTPTXT, NT.ISDEL 
FROM "INC_T_NRMTP" AS NT
INNER JOIN "INC_T_INCDT" AS INC ON INC.INCID = NT.INCID AND INC.ISDEL = '0' AND NT.ISDEL = '0'
LEFT JOIN "INC_M_MSTDT" AS MD ON MD.UNQID = NT.NRMTP AND MD.TYVAL = 'Type of Near Miss' AND MD.ISDEL='0'
WHERE NT.ISDEL='0';